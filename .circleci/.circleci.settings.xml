<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>bintray-mcollovati-maven-repo</id>
            <username>mcollovati</username>
            <password>${env.BINTRAY_TOKEN}</password>
        </server>
        <server>
            <id>bintray-oss-snapshots</id>
            <username>mcollovati</username>
            <password>${env.BINTRAY_TOKEN}</password>
        </server>
        <server>
            <id>jfrog-vertx-vaadin-releases</id>
            <username>${env.JFROG_USER}</username>
            <password>${env.JFROG_TOKEN}</password>
        </server>
        <server>
            <id>jfrog-vertx-vaadin-snapshots</id>
            <username>${env.JFROG_USER}</username>
            <password>${env.JFROG_TOKEN}</password>
        </server>
        <server>
            <id>gpg.passphrase</id>
            <passphrase>${env.MAVEN_GPG_PASSPHRASE}</passphrase>
        </server>
    </servers>
</settings>
